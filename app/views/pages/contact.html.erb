      </div>
    </div>
  </div>


  <div class="grid-padding" style="">
  <%=@contact_cat[0]%>
  <% @contact_cat[1..-1].each do |item|%>
  <li><%=item%></li>
  <%end%>
  </div>


</div>

<script>
function resizeMap() {
header = document.querySelector("header");
navbar = document.querySelector(".navbar");
footer = document.querySelector("footer");
iframe = document.querySelector("iframe");
iframe.style.height = `${(window.innerHeight - header.offsetHeight - footer.offsetHeight - navbar.offsetHeight)}px`;
}


window.onload = resizeMap()
window.addEventListener("resize", resizeMap);

</script>
